{
  "name": "highcharts-export-server",
  "author": "Highsoft AS <support@highcharts.com> (http://www.highcharts.com/about)",
  "repository": {
    "url": "https://github.com/highcharts/node-export-server",
    "type": "git"
  },
  "version": "3.0.0",
  "bin": {
    "highcharts-export-server": "./bin/cli.js"
  },
  "scripts": {
    "install": "node install.js",
    "start": "node bin/cli.js --enableServer 1",
    "lint": "eslint ./",
    "cli-tests": "node tests/cli/cli_test_runner.js",
    "http-tests": "node tests/http/http_test_runner.js",
    "node-tests": "node tests/node/node_test_runner.js"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "colors": "1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-form-data": "^2.0.17",
    "express-rate-limit": "^6.3.0",
    "generic-pool": "^3.8.2",
    "https-proxy-agent": "^5.0.1",
    "node-fetch": "^2.6.7",
    "prompts": "^2.4.2",
    "puppeteer": "^14.1.1",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npx eslint --cache --fix",
    "*.{js,css,md}": "npx prettier --write"
